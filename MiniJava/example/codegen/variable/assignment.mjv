class Main {
    public static void main(String[] args) {
    	{
    		System.out.println(new A().normalassign());
    		System.out.println(new A().arrayassign());
    	}
    }
}

class A {
	public int normalassign() {
		int ret;
		ret = 1;
		return ret;
	}
	
	public int arrayassign() {
		int[] arr;
		arr = new int[5];
		arr[2] = 1;
		return arr[2];
	}
}