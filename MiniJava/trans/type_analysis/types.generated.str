  create-type-task(|ctx) :
    UnExp(operator, exp) -> <type-is(|ctx, [ety1, t37, ty1, ty, eq181])> ty
    where ety1 := <type-task(|ctx)> exp;
          t37 := <type-task(|ctx)> operator;
          ty1 := <new-task(|ctx)> Rewrite("proj93", t37);
          ty := <new-task(|ctx)> Rewrite("proj94", t37);
          eq181 := <type-match(|ctx, ty1)> ety1;
          <task-create-error-on-failure(|ctx, eq181, "Incompatible type")> exp
  
  create-type-task(|ctx) :
    Negation() -> <type-is(|ctx, [])> (Bool(), Bool())
    where id
  
  create-type-task(|ctx) :
    ArrayLength() -> <type-is(|ctx, [])> (IntArray(), Int())
    where id
  
  create-type-task(|ctx) :
    AllocateArray() -> <type-is(|ctx, [])> (Int(), IntArray())
    where id
  
  create-type-task(|ctx) :
    BinExp(operator, subexp1, subexp2) -> <type-is(|ctx, [ety1, ety2, t38, ty1, ty2, ty, eq182, eq183])> ty
    where ety1 := <type-task(|ctx)> subexp1;
          ety2 := <type-task(|ctx)> subexp2;
          t38 := <type-task(|ctx)> operator;
          ty1 := <new-task(|ctx)> Rewrite("proj90", t38);
          ty2 := <new-task(|ctx)> Rewrite("proj91", t38);
          ty := <new-task(|ctx)> Rewrite("proj92", t38);
          eq182 := <type-match(|ctx, ty1)> ety1;
