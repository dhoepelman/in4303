          texp := <type-task(|ctx)> exp;
          eq187 := <type-match(|ctx, t)> texp;
          <task-create-error-on-failure(|ctx, eq187, "Incompatible variable and expression type")> exp
  
  nabl-constraint(|ctx) :
    Print(exp) -> <fail>
    where texp := <type-task(|ctx)> exp;
          eq188 := <type-match(|ctx, Int())> texp;
          <task-create-error-on-failure(|ctx, eq188, "Incompatible expression type: can only print integers")> exp
  
  nabl-constraint(|ctx) :
    While(exp, statements) -> <fail>
    where texp := <type-task(|ctx)> exp;
          eq189 := <type-match(|ctx, Bool())> texp;
          <task-create-error-on-failure(|ctx, eq189, "Incompatible expression type: condition must be boolean")> exp
  
  nabl-constraint(|ctx) :
    If(exp, ifstmt, elsestmt) -> <fail>
    where texp := <type-task(|ctx)> exp;
          eq190 := <type-match(|ctx, Bool())> texp;
          <task-create-error-on-failure(|ctx, eq190, "Incompatible expression type: condition must be boolean")> exp
  
  nabl-constraint(|ctx) :
