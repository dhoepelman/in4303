          eq346 := <type-match(|ctx, t)> texp;
          <task-create-error-on-failure(|ctx, eq346, "Incompatible variable and expression type")> exp
  
  nabl-constraint(|ctx) :
    Print(exp) -> <fail>
    where texp := <type-task(|ctx)> exp;
          eq347 := <type-match(|ctx, Int())> texp;
