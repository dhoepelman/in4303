  nabl-constraint(|ctx) :
    ArrayAssign(var, idx, exp) -> <fail>
    where tvar := <type-lookup(|ctx)> var;
          tidx := <type-task(|ctx)> idx;
          texp := <type-task(|ctx)> exp;
          eq343 := <type-match(|ctx, IntArray())> tvar;
          <task-create-error-on-failure(|ctx, eq343, "Not an array")> var;
          eq344 := <type-match(|ctx, Int())> tidx;
          <task-create-error-on-failure(|ctx, eq344, "Array index needs to be an integer")> idx;
          eq345 := <type-match(|ctx, Int())> texp;
          <task-create-error-on-failure(|ctx, eq345, "Incompatible array and expression type")> exp
  
