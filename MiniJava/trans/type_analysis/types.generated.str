  nabl-constraint(|ctx) :
    ArrayAssign(var, idx, exp) -> <fail>
    where tvar := <type-lookup(|ctx)> var;
          tidx := <type-task(|ctx)> idx;
          texp := <type-task(|ctx)> exp;
          eq184 := <type-match(|ctx, IntArray())> tvar;
          <task-create-error-on-failure(|ctx, eq184, "Not an array")> var;
          eq185 := <type-match(|ctx, Int())> tidx;
          <task-create-error-on-failure(|ctx, eq185, "Array index needs to be an integer")> idx;
          eq186 := <type-match(|ctx, Int())> texp;
          <task-create-error-on-failure(|ctx, eq186, "Incompatible array and expression type")> exp
  
